<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coroutinesandmultithreadingimplementation">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".menu.MenuActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

<!--    MultiThreading Implementation    -->
        <activity
            android:name=".androidmultithreadingmasterclass.MainActivity"
            android:exported="true" />

<!--    Coroutines Implementation 1 start    -->
        <activity
            android:name=".androidcoroutinesmasterclass.CoroutineMainActivity"
            android:exported="true" />
<!--    Coroutines Implementation 1 end    -->

    </application>

</manifest>